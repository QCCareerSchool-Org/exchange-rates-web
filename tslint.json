{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "object-literal-sort-keys": false,
        "curly": [true, "as-needed"],
        "array-bracket-spacing": [true, "always"]
    },
    "rulesDirectory": []
}